; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="WalletAdapterAndroid"
run/main_scene="res://wallet_adapter_android.tscn"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[autoload]

SolanaService="*res://addons/SolanaSDK/Autoload/SolanaService.tscn"
HttpRequestHandler="*res://addons/SolanaSDK/Autoload/HttpRequestHandler.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/SolanaSDK/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
